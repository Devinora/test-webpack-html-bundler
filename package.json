{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Front End Template",
  "private": true,
  "scripts": {
    "dev": "webpack serve --node-env development --config ./configs/webpack.dev.js",
    "build": "webpack --node-env development --config ./configs/webpack.dev.js",
    "prod": "webpack --node-env production --config ./configs/webpack.prod.js",
    "watch": "webpack --watch --node-env watch --env watch --config ./configs/webpack.prod.js",
    "analyze": "webpack --node-env production --config ./configs/webpack.prod.js --analyze"
  },
  "keywords": [
    "webpack",
    "js",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devinora/webpack-template.git"
  },
  "author": "Devinora <kh.devinora@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Devinora/webpack-template/issues"
  },
  "homepage": "https://github.com/Devinora/webpack-template#readme",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "ejs":"3.1.8",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "html-bundler-webpack-plugin": "^1.2.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
